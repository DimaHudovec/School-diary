<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <TabHost
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/tabHost" android:layout_gravity="center_horizontal">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
            <TabWidget
                    android:id="@android:id/tabs"
                    android:layout_width="match_parent"
                    android:layout_height="0dp" android:layout_weight="1">
            </TabWidget>
            <FrameLayout
                    android:id="@android:id/tabcontent"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:layout_weight="1">
                <LinearLayout
                        android:id="@+id/linearLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <ImageButton
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:id="@+id/imageButton2" android:layout_gravity="top|right"
                            android:background="@android:drawable/ic_menu_today" android:onClick="calendar"/>
                    <TableLayout android:layout_width="match_parent" android:layout_height="match_parent"
                                 android:layout_above="@+id/button2" android:layout_toStartOf="@+id/button"
                                 android:layout_alignEnd="@+id/button" android:layout_below="@+id/imageButton">
                        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
                            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="1"
                                      android:id="@+id/textView12" android:layout_column="0"
                                      android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                                      android:textAppearance="?android:attr/textAppearanceLarge"
                                      android:id="@+id/textView15" android:layout_column="1"/>
                            <EditText android:layout_width="150dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText15" android:layout_column="2" android:singleLine="true"/>
                            <EditText android:layout_width="35dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText16" android:layout_column="3"
                                      android:inputType="numberDecimal" android:editable="false" android:maxLength="2"/>
                            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                                      android:id="@+id/checkBox8" android:layout_column="4" android:checked="false"/>
                        </TableRow>
                        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
                            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="2"
                                      android:id="@+id/textView16" android:layout_column="0"
                                      android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                                      android:textAppearance="?android:attr/textAppearanceLarge"
                                      android:id="@+id/textView17" android:layout_column="1"/>
                            <EditText android:layout_width="150dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText17" android:layout_column="2" android:singleLine="true"/>
                            <EditText android:layout_width="35dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText18" android:layout_column="3"
                                      android:inputType="numberDecimal" android:maxLength="2"/>
                            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                                      android:id="@+id/checkBox9" android:layout_column="4" android:checked="false"/>
                        </TableRow>
                        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
                            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="3"
                                      android:id="@+id/textView18" android:layout_column="0"
                                      android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                                      android:textAppearance="?android:attr/textAppearanceLarge"
                                      android:id="@+id/textView19" android:layout_column="1"/>
                            <EditText android:layout_width="150dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText19" android:layout_column="2" android:singleLine="true"/>
                            <EditText android:layout_width="35dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText20" android:layout_column="3"
                                      android:inputType="numberDecimal" android:maxLength="2"/>
                            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                                      android:id="@+id/checkBox10" android:layout_column="4" android:checked="false"/>
                        </TableRow>
                        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
                            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="4"
                                      android:id="@+id/textView20" android:layout_column="0"
                                      android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                                      android:textAppearance="?android:attr/textAppearanceLarge"
                                      android:id="@+id/textView21" android:layout_column="1"/>
                            <EditText android:layout_width="150dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText21" android:layout_column="2" android:singleLine="true"/>
                            <EditText android:layout_width="35dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText22" android:layout_column="3"
                                      android:inputType="numberDecimal" android:maxLength="2"/>
                            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                                      android:id="@+id/checkBox11" android:layout_column="4" android:checked="false"/>
                        </TableRow>
                        <TableRow android:layout_width="150dp" android:layout_height="match_parent">
                            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="5"
                                      android:id="@+id/textView22" android:layout_column="0"
                                      android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                            <TextView
                                    android:layout_width="120dp"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceLarge"
                                    android:id="@+id/textView" android:layout_column="1"/>
                            <EditText android:layout_width="150dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText23" android:layout_column="2" android:singleLine="true"/>
                            <EditText android:layout_width="35dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText24" android:layout_column="3"
                                      android:inputType="numberDecimal" android:maxLength="2"/>
                            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                                      android:id="@+id/checkBox12" android:layout_column="4" android:checked="false"/>
                        </TableRow>
                        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
                            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="6"
                                      android:id="@+id/textView23" android:layout_column="0"
                                      android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                                      android:textAppearance="?android:attr/textAppearanceLarge"
                                      android:id="@+id/textView24" android:layout_column="1"/>
                            <EditText android:layout_width="150dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText25" android:layout_column="2" android:singleLine="true"/>
                            <EditText android:layout_width="35dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText26" android:layout_column="3"
                                      android:inputType="numberDecimal" android:maxLength="2"/>
                            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                                      android:id="@+id/checkBox13" android:layout_column="4" android:checked="false"/>
                        </TableRow>
                        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
                            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="7"
                                      android:id="@+id/textView25" android:layout_column="0"
                                      android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                                      android:textAppearance="?android:attr/textAppearanceLarge"
                                      android:id="@+id/textView26" android:layout_column="1"/>
                            <EditText android:layout_width="150dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText27" android:layout_column="2" android:singleLine="true"/>
                            <EditText android:layout_width="35dp" android:layout_height="wrap_content"
                                      android:id="@+id/editText28" android:layout_column="3"
                                      android:inputType="numberDecimal" android:maxLength="2"/>
                            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                                      android:id="@+id/checkBox14" android:layout_column="4" android:checked="false"/>
                        </TableRow>
                    </TableLayout>
                    <Button
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Добавить"
                            android:id="@+id/button3" android:layout_gravity="center_horizontal"/>
                </LinearLayout>
                <LinearLayout
                        android:id="@+id/linearLayout2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                        android:id="@+id/linearLayout3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </TabHost>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Выход"
            android:id="@+id/button4" android:layout_gravity="bottom|right"/>
</LinearLayout>